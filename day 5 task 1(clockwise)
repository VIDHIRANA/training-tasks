/* Write a method void printArray(int[][]arr)
Prints array elements clock wise and anti-clockwise alternatively. */

// C++. program for the above approach
#include <iostream>
#include <bits/stdc++.h>

using namespace std;

void print(){
  int n,m;
  cout << "\nInput the number of rows : ";
  cin >> m;
  cout << "\nInput the number of columns : ";
  cin >> n;
  int arr[m][n];
  int i,j;
  cout << "\nInput the matrix \n";
  for(i = 0; i < m; i++)
  {
    for(j = 0; j < n; j++)
    {
      cin >> arr[i][j];
    }
  }
 
    cout << "\n clockwise Matrix : ";
    int k = 0, l = 0;
    while(k < m && l < n)
    {
      for(i = l; i < n; i++)
      {
        cout << arr[k][i] << " ";
      }
      k++;
      for(i = k; i < m; i++)
      {
        cout << arr[i][n-1] << " ";
      }
      n=n-1;
      if(k < m)
      {
        for(i = n - 1; i >= 0; --i)
        {
          cout << arr[m-1][i] << " ";
        }
        m=m-1;
      }
      if(l < n)
      {
        for(i = m - 1; i >= k; i--)
        {
          cout << arr[i][l] << " ";
        }
        l++;
      }
    }
    
    cout << endl;
  
  
 
   
   

}



// Driver Code
int main()
{

	// Function Call
	print();
  
	return 0;
}

